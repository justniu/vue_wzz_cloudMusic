\n            //点击歌单跳转界面\r\n            toSongListPage(id) {\r\n                this.$router.push('/songlist/' + id)\r\n            },\r\n            //双击歌曲添加数据进去,并循环播放(向父组件传递参数)\r\n            playMusicList(row) {\r\n                this.$emit('setSongListInfo', window.localStorage.getItem('playlist'), row.id)\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}